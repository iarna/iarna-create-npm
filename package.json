{
  "name": "@iarna/create-npm",
  "version": "1.0.1",
  "description": "Init the current project using @iarna's npm defaults.",
  "author": {
    "name": "Rebecca Turner",
    "email": "me@re-becca.org",
    "twitter": "ReBeccaOrg"
  },
  "license": "ISC",
  "scripts": {
    "prerelease": "npm t",
    "release": "standard-version -s",
    "postrelease": "npm publish && git push --follow-tags",
    "test": "standard",
    "update-coc": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'"
  },
  "devDependencies": {
    "standard": "*",
    "standard-version": "*",
    "weallbehave": "*",
    "weallcontribute": "*"
  },
  "bin": "init.js",
  "files": [
    "init.js"
  ],
  "bundleDependencies": [
    "@iarna/create"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iarna/iarna-create-npm.git"
  },
  "bugs": "https://github.com/iarna/iarna-create-npm/issues",
  "homepage": "https://npmjs.com/package/iarna-create-npm",
  "dependencies": {
    "@iarna/create": "^1.1.1"
  }
}
